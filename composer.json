{
    "name": "izie/kinvey-php",
    "description": "Kinvey PHP SDK",
    "keywords": [
        "izie",
        "kinvey",
        "sdk",
        "php"
    ],
    "homepage": "https://github.com/italoqueiroz/kinvey-php",
    "license": "MIT",
    "authors": [
        {
            "name": "Ítalo Queiroz",
            "email": "italoqueiroz@gmail.com",
            "homepage": "http://italoqueiroz.github.io/",
            "role": "Developer"
        }
    ],
    "require": {
        "php" : ">=5.3.0"
    },
    "require-dev": {
        "phpunit/phpunit" : "4.*"
    },
    "autoload": {
        "psr-4": {
            "Izie\\Kinvey\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Izie\\Kinvey\\Test\\": "tests"
        }
    },
    "extra": {
        "branch-alias": {
            "dev-master": "1.0-dev"
        }
    }
}
